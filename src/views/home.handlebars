<div class="container">
    <h1>Home</h1>
    <h2>Products list</h2>
    <ul>
        {{#each products}}
            <li>
                <h2>{{title}}</h2>
                <p>Description: {{description}}</p>
                <p>Price: {{price}}</p>
                <img src={{thumbnail}} alt={{title}}>
            </li>
        {{/each}}
    </ul>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/main.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>